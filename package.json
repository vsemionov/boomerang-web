{
  "name": "boomerang-web",
  "version": "0.2.8",
  "description": "Boomerang front-end",
  "author": "Victor Semionov <vsemionov@gmail.com>",
  "private": true,
  "scripts": {
    "build": "run-s build:**",
    "build:js": "cross-env NODE_ENV=production rollup -c",
    "build:css": "nodetouch www/boomerang/boomerang.css",
    "build:autoprefixer": "postcss --use autoprefixer -o www/boomerang/boomerang.css www/boomerang/boomerang.css",
    "build:cssnano": "cssnano www/boomerang/boomerang.css www/boomerang/boomerang.css",

    "start": "cross-env NODE_ENV=development rollup -cw",

    "postinstall": "npm run build"
  },
  "dependencies": {
    "vue": "2.4.2",
    "vue-router": "2.7.0",

    "raven-js": "3.17.0",

    "axios": "0.16.2",
    "jwt-decode": "2.2.0",

    "cross-env": "5.0.5",
    "npm-run-all": "4.1.1",
    "postcss-cli": "4.1.0",
    "autoprefixer": "7.1.3",
    "cssnano-cli": "1.0.5",
    "touch": "3.1.0",

    "rollup": "0.49.2",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-alias": "1.3.1",
    "rollup-plugin-vue": "2.4.1",
    "rollup-plugin-json": "2.3.0",
    "rollup-plugin-buble": "0.15.0",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-commonjs": "8.2.0",
    "rollup-plugin-node-globals": "1.1.0",
    "rollup-plugin-uglify": "2.0.1",
    "rollup-plugin-livereload": "0.6.0",
    "rollup-plugin-serve": "0.4.2",
    "rollup-watch": "4.3.1"
  },
  "devDependencies": {
  },
  "engines": {
    "node": "6.11.3"
  }
}
