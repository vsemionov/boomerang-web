{
  "name": "boomerang-web",
  "version": "0.2.10",
  "description": "Boomerang front-end",
  "author": "Victor Semionov <vsemionov@gmail.com>",
  "private": true,
  "scripts": {
    "build": "run-s build:**",
    "build:js": "cross-env NODE_ENV=production rollup -c",
    "build:css": "nodetouch www/boomerang.css",
    "build:autoprefixer": "postcss --use autoprefixer -o www/boomerang.css www/boomerang.css",
    "build:cssnano": "cssnano www/boomerang.css www/boomerang.css",
    "build:bootstrap": "cpx node_modules/bootstrap/dist/** www/bootstrap",
    "build:jquery": "cpx node_modules/jquery/dist/** www/jquery",
    "build:bootstrap-social": "cpx node_modules/bootstrap-social/bootstrap-social.css www/bootstrap-social/css",
    "build:font-awesome": "cpx node_modules/font-awesome/{css,fonts}/** www/font-awesome",

    "start": "cross-env NODE_ENV=development rollup -cw",

    "postinstall": "npm run build"
  },
  "dependencies": {
    "vue": "2.4.4",
    "vue-router": "2.7.0",

    "raven-js": "3.17.0",

    "bootstrap": "3.3.7",
    "jquery": "1.12.4",
    "bootstrap-social": "5.1.1",
    "font-awesome": "4.7.0",

    "axios": "0.16.2",
    "vue-axios": "2.0.2",
    "jwt-decode": "2.2.0",

    "cross-env": "5.0.5",
    "npm-run-all": "4.1.1",
    "postcss-cli": "4.1.1",
    "autoprefixer": "7.1.4",
    "cssnano-cli": "1.0.5",
    "touch": "3.1.0",
    "cpx": "1.5.0",

    "rollup": "0.49.3",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-alias": "1.3.1",
    "rollup-plugin-vue": "2.4.2",
    "rollup-plugin-json": "2.3.0",
    "rollup-plugin-buble": "0.15.0",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-commonjs": "8.2.1",
    "rollup-plugin-node-globals": "1.1.0",
    "rollup-plugin-uglify": "2.0.1",
    "rollup-plugin-livereload": "0.6.0",
    "rollup-plugin-serve": "0.4.2",
    "rollup-watch": "4.3.1"
  },
  "devDependencies": {
  },
  "engines": {
    "node": "6.11.3"
  }
}
